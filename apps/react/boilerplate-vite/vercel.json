{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "dist/server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/server.js",
      "headers": {
        "cache-control": "s-maxage=7200, max-age=86400, stale-while-revalidate"
      }
    }
  ]
}