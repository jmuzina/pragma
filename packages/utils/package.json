{
  "name": "@canonical/utils",
  "description": "Standard utility functions for Canonical's Web Engineering team",
  "version": "0.10.0-experimental.5",
  "type": "module",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
